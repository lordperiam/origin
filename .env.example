# DB
# PostgreSQL database connection URL
# Example: postgres://user:password@localhost:5432/dbname
DATABASE_URL=

# Supabase
# Supabase project URL
# Example: https://your-project.supabase.co
SUPABASE_URL=
# Supabase service role key for server-side operations
SUPABASE_SERVICE_ROLE_KEY=
# Supabase bucket name for storing receipts or other files
SUPABASE_BUCKET_RECEIPTS=

# Auth
# Clerk publishable key for frontend authentication
# Obtain from Clerk dashboard
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
# Clerk secret key for backend authentication
# Obtain from Clerk dashboard
CLERK_SECRET_KEY=
# Clerk sign-in URL
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/login
# Clerk sign-up URL
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/signup

# Payments
# Stripe secret key for processing payments
# Obtain from Stripe dashboard
