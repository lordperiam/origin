# Stripe webhook secret for handling subscription events
# Obtain from Stripe dashboard
STRIPE_WEBHOOK_SECRET=
# Stripe payment link for yearly subscription
# Create in Stripe dashboard
NEXT_PUBLIC_STRIPE_PAYMENT_LINK_YEARLY=
# Stripe payment link for monthly subscription
# Create in Stripe dashboard
NEXT_PUBLIC_STRIPE_PAYMENT_LINK_MONTHLY=

# AI
# OpenAI API key for AI services (e.g., transcript generation, analysis)
# Obtain from OpenAI dashboard
